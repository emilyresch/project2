<h1>Filler</h1>

<div class="mdc-text-field mdc-text-field--outlined" id="inputText">
  <input class="mdc-text-field__input" id="text-field-hero-input bookSearch">
  <div class="mdc-notched-outline">
    <div class="mdc-notched-outline__leading"></div>
    <div class="mdc-notched-outline__notch">
      <label for="text-field-hero-input" class="mdc-floating-label">Search Book</label>
    </div>
    <div class="mdc-notched-outline__trailing"></div>
  </div>
</div>

<h3>Search Results</h3>

<div class="mdc-layout-grid">
    {{#each books}}
    <div class="mdc-layout-grid__inner">
        <div class="mdc-card mdc-card--outlined" style="padding:16px; height:350px; width:75%; margin:auto;">
            <h5>{{this.volumeInfo.title}}</h5>
            <h6>{{this.volumeInfo.authors}}</h6>
            <p>{{this.volumeInfo.description}}</p>
            <a href="{{this.saleInfo.buyLink}}">Buy Book</a>
            <div class="mdc-card__action-icons">
            <button class="material-icons mdc-icon-button mdc-card__action mdc-card__action--icon" 
                aria-label="Mark as Read">bookmark_border</button>
            <button class="material-icons mdc-icon-button mdc-card__action mdc-card__action--icon"
                aria-label="Wishlist">star_border</button>
            <button class="material-icons mdc-icon-button mdc-card__action mdc-card__action--icon"
                aria-label="Favorite">favorite_border</button>
        </div>
        </div>
    </div>
    {{/each}}
</div>








